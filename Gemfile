# A sample Gemfile
source "http://rubygems.org"

gem 'sinatra'
gem 'sinatra-config-file', '~>0.6.1', :require => 'sinatra/config_file'
gem 'sinatra-compass', :require => 'sinatra/compass'
gem 'jim'
gem 'redis'
gem 'redis-namespace'
gem 'compass'

group :development do
  gem 'sinatra-reloader', :require => 'sinatra/reloader'
  gem 'thin'
  gem 'ruby-debug19'
end
